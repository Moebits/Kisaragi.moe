{
  "name": "kisaragi-site-react",
  "version": "0.0.1",
  "description": "Kisaragi Bot's website built with React.",
  "main": "dist/server.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "npm run build && npm run web",
    "web": "node --max_old_space_size=4096 dist/server.js",
    "build": "npm run clean && npm run bundle-web && npm run bundle-server && npm run redirects",
    "bundle-web": "webpack --env platform=web",
    "bundle-server": "webpack --env platform=server",
    "redirects": "echo \"/* /index.html 200\" | cat > dist/_redirects",
    "clean": "del-cli ./dist"
  },
  "homepage": ".",
  "engines": {
    "node": "16.6.2"
  },
  "keywords": [
    "Kisaragi",
    "Bot",
    "Discord",
    "Moe",
    "Cute",
    "React"
  ],
  "author": "Tenpi",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/jquery": "^3.5.1",
    "@types/less": "^3.0.1",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-router-hash-link": "^1.2.1",
    "@types/webpack-env": "^1.15.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "del-cli": "^3.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "gh-pages": "^5.0.0",
    "helmet": "^4.0.0",
    "history": "^5.0.0",
    "html-react-parser": "^0.13.0",
    "jquery": "^3.5.1",
    "less": "^3.12.2",
    "mime": "^2.4.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.0.0",
    "react-slide-toggle": "^0.3.5",
    "react-string-replace": "^0.4.4",
    "reactjs-localstorage": "^1.0.1",
    "renamer": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@tenpi/tslint": "^1.0.9",
    "css-loader": "^6.10.0",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "less-loader": "^12.2.0",
    "mini-css-extract-plugin": "^2.8.1",
    "raw-loader": "^4.0.2",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "tslint": "^6.1.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.1.0",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-node-externals": "^3.0.0"
  }
}
